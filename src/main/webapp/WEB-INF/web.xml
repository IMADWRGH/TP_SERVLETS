<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
<context-param>
  <param-name>driver</param-name>
  <param-value>com.mysql.jdbc.Driver</param-value>
</context-param>
<context-param>
  <param-name>url</param-name>
  <param-value>jdbc:mysql://localhost/db_biblio</param-value>
</context-param>
<context-param>
  <param-name>user</param-name>
  <param-value>root</param-value>
</context-param>
<context-param>
  <param-name>password</param-name>
  <param-value></param-value>
</context-param>
<servlet>
  <servlet-name>ControleurDictionnaire</servlet-name>
  <servlet-class>Controleur.ControleurDictionnaire</servlet-class>
</servlet>
<servlet>
  <servlet-name>ControleurLivre</servlet-name>
  <servlet-class>Controleur.ControleurLivre</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>ControleurDictionnaire</servlet-name>
  <url-pattern>/Dictionnaire</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>ControleurLivre</servlet-name>
  <url-pattern>/livre</url-pattern>
</servlet-mapping>
<session-config>
  <session-timeout>
    30
  </session-timeout>
</session-config>
</web-app>